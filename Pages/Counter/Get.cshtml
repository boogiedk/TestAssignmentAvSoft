@page
@model TestAssignmentAvSoft.Pages.Counter.GetModel

@{
    ViewData["Title"] = "Get";
}

<h2>Get</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CounterModel.FirstOrDefault().Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CounterModel.FirstOrDefault().Value)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.CounterModel) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
        </tr>
}
    </tbody>
</table>
